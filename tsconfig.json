{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./*"],
      "~/*": ["src/*"],
      "app/*": ["src/app/*"],
      "@/constants/*": ["constants/*"],
      "~/components/*": ["./src/components/*"],
      "~/utils/*": ["src/utils/*"]
    }
  }
}
